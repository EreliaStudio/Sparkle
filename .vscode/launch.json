{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Launch Debug",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/build/debug/out/bin/Debug/x64/Playground.exe",
		"args": [],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
		  {
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		  }
		],
		"preLaunchTask": "Build Debug",
		"miDebuggerPath": "gdb", // Update this path to your gdb executable
		"miDebuggerArgs": "",
		"logging": {
		  "engineLogging": true
		}
	  }
	]
  }